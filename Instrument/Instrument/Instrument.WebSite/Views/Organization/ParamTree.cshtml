@{
    ViewBag.Title = "ParamTreePage";
    Layout = "~/Views/Shared/_LayoutTree.cshtml";
}
<script type="text/javascript" language="javascript">
        $(document).ready(function () {
            iniTree();
           
            function iniTree() { 
                tree = new dhtmlXTreeObject("treeboxbox_tree", "100%", "100%", 0);
                tree.setImagePath("@Url.Content("~/Content/themes/base/images/csh_scbrblue/")");
                tree.setOnClickHandler(func_click);
                tree.enableSmartXMLParsing(true);
                tree.loadXML("/Organization/LoadBelongParam4dhtmlxTree?ajax=true");
            }

            function func_click() {
                var selectName = tree.getSelectedItemText();
                window.parent.ymPrompt.close();
                window.parent.fnGetSelectOrgName(selectName);
            }   

        });
</script>
<div id="treeboxbox_tree" style="width: 250; height: 218; background-color: #f5f5f5;
    border: 1px solid Silver; overflow: auto;">
</div>

