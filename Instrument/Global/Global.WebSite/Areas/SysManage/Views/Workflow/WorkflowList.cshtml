@{
    ViewBag.Title = "Workflow";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript" language="javascript">
    $(document).ready(function () {
//        GetJsondataFromServ("todo", "/SysManage/Workflow/GetJsonTodoAuditList?userName=" + $("#txtUserName").val() + "&workflows=" + $("#txtWorkflows").val());
        var data = @Html.Raw(ViewBag.todoData);           
        var oTable = WrapTables4JsArray("todo", data);
        oTable.fnSort( [[1,'desc'],[3,'desc']] );
       
    });   
</script>
@*<input type="hidden" id="txtUserName" value="@ViewBag.UserName" />
<input type="hidden" id="txtWorkflows" value="@ViewBag.Workflows" />*@
<table cellpadding="0" cellspacing="0" border="0" class="display" id="todo">
    <thead>
        <tr>           
            <th width="50px;">
                审核
            </th>
            <th width="350px;">
                工单
            </th>
            <th>
                状态
            </th>
            <th>
                当前步骤
            </th>
            <th>
                发起人
            </th>
            <th width="150px">
                发起时间
            </th>
           
        </tr>
    </thead>
    <tbody>
        <tr>
